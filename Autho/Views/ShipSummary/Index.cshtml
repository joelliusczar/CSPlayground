@model IEnumerable<Autho.Models.Ship>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table id="example" class="display" style="width:100%">
    <thead>
        <tr>
            <th>name</th>
            <th>CaptainName</th>
            <th>NumCannons</th>
        </tr>
    </thead>
    <tfoot>
        <tr>
            <th>name</th>
            <th>CaptainName</th>
            <th>NumCannons</th>
        </tr>
    </tfoot>
</table>

<script>
    $(document).ready(function () {
        $('#example').DataTable({
            "processing": true,
            "serverSide": true,
            "ajax": "ShipSummary/GetJason",
            "columns": [
                { "data": "name" },
                { "data": "CaptainName" },
                { "data": "NumCannons" },
 
            ]
        });
    });
</script>
